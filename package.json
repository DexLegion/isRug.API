    "@jpmonette/bscscan": "^0.2.4",
    "@solidity-parser/parser": "^0.16.0",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "ethers": "^5.7.1",
    "evm": "^0.2.0",
    "express-slow-down": "^1.4.0",
    "fastify": "^4.1.0",
    "ganache": "^7.3.2",
    "https-proxy-agent": "^5.0.1",
