    "@types/jest": "^29.5.1",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.0.0",
    "@types/ws": "^8.5.4",
    "jest": "^29.5.0",
    "regenerator-runtime": "^0.13.9",
    "solc": "0.8.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "^4.8.3"
  }
